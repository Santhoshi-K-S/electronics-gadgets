{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/server/(.*)",
        "dest": "server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "build/$1"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
  